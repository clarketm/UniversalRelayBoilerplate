      "eslint --fix",
      "prettier-eslint --write",
      "git add"
    ],
    "units/**/*.jsx": [
      "eslint --fix",
      "prettier-eslint --write",
      "git add"
    ],
    "units/**/*.json": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "name": "UniversalRelayBoilerplate",
  "prettier": {
    "bracketSpacing": true,
    "printWidth": 100,
    "parser": "flow",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "scripts": {
    "build-bump-version": "./node_modules/.bin/babel-node ./units/urb-base-tools/build-bump-version.js",
    "build-bundle-android": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --sourcemap-output android/app/src/main/assets/index.android.map --assets-dest android/app/src/main/res/",
    "build-bundle-android-dev": "react-native bundle --platform android --dev true --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --sourcemap-output android/app/src/main/assets/index.android.map --assets-dest android/app/src/main/res/",
    "build-bundle-ios": "react-native bundle --platform ios --dev false --entry-file index.ios.js --bundle-output ios/main.jsbundle --sourcemap-output ios/main.map",
    "build-bundle-ios-dev": "react-native bundle --platform ios --dev true --entry-file index.ios.js --bundle-output ios/main.jsbundle --sourcemap-output ios/main.map",
    "build-deployment": "npm run build-relay && npm run build-server && npm run build-webpack",
    "build-relay": "./node_modules/.bin/babel-node ./units/urb-base-tools/build-units.js && ./node_modules/.bin/babel-node ./units/urb-base-server/build-relay-schema.js && ./node_modules/.bin/relay-compiler --src ./units/ --extensions=js jsx --schema units/_configuration/urb-base-server/graphql/schema.graphql",
    "build-server": "export NODE_ENV=production && rm -rf deployment/* && cp -r ./units/_configuration ./deployment/units && ./node_modules/.bin/babel -s true --retain-lines -d ./deployment/units/ ./units && rsync -avm --include='*.ejs' -f 'hide,! */' units ./deployment/ && rsync -avm --include='*.graphql.js' -f 'hide,! */' units ./deployment/ && cp ./package.json ./deployment && cp ./yarn.lock ./deployment",
    "build-units": "./node_modules/.bin/babel-node ./units/urb-base-tools/build-units.js",
    "build-webpack": "export NODE_ENV=production && rm -rf deployment/units/_configuration/urb-base-server/public_files/assets/* &&  ./node_modules/.bin/webpack -p --output-filename client.js",
    "dev": "./node_modules/.bin/concurrently --kill-others \"npm run dev-relay\" \"npm run dev-webpack\" \"npm run dev-server\" ",
    "dev-app": "./node_modules/.bin/concurrently --kill-others \"npm run dev-relay\" \"npm run dev-webpack\" \"npm run dev-server\" \"npm run start\" ",
    "dev-prod": "export NODE_ENV=production && ./node_modules/.bin/babel-node ./units/urb-base-server/server.js",
    "dev-relay": "./node_modules/.bin/relay-compiler --src ./units/ --extensions=js jsx --schema units/_configuration/urb-base-server/graphql/schema.graphql --watch",
    "dev-reset": "sudo chmod -R a+w * && rm -rf ~/Library/Developer/Xcode/DerivedData/* && rm -f ./ios/main.* && rm -f ./android/app/src/main/assets/index.android.* && watchman watch-del-all &&  rm -rf $TMPDIR/react-* && rm -rf ~/.node-gyp  && (cd units && find . -type d -name '__generated__' | xargs rm -r)",
    "dev-server": "export NODE_ENV=development && ./node_modules/.bin/nodemon --delay 5 -e js,jsx --exec ./node_modules/.bin/babel-node -- ./units/urb-base-server/server.js",
    "dev-webpack": "./node_modules/.bin/webpack-dev-server --hot --inline --no-info --colors",
    "postinstall": "node ./units/urb-example-inscriptio-webapp/fix-react-rte.js",
    "_precommit": "lint-staged",
    "setup-database": "./node_modules/.bin/babel-node ./units/urb-base-tools/setup-database.js",
    "setup-local": "cp ./units/_configuration/default.env ./.env && cp ./units/_configuration/urb-base-app/default.publicURL.js ./units/_configuration/urb-base-app/publicURL.js && npm run update-ip && react-native link",
    "setup-local-cut-version": "cp ./units/_configuration/default.env ./.env && cp ./units/_configuration/urb-base-app/default.publicURL.js ./units/_configuration/urb-base-app/publicURL.js",
    "start": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "update-default-persister": "./node_modules/.bin/babel-node ./units/urb-base-tools/update-default-persister.js",
    "update-ip": "./node_modules/.bin/babel-node ./units/urb-base-tools/update-ip.js"
  },
  "version": "0.4.1"
}
